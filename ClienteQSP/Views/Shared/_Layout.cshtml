<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Quien se pega</title>
    @Styles.Render("~/Content/materialize")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>

    <ul id="slide-out" class="side-nav" id="side-nav">
        <li>
            <div class="user-view">
                <div class="background">
                    <img src="@Url.Content("~/img/Unipanamericana.jpg")">
                </div>
                <a href="#!user"><img class="circle" src="@Url.Content("~/img/avatar.jpg")"></a>
                <a href="#!name"><span class="white-text name">John Doe</span></a>
                <a href="#!email"><span class="white-text email">jdandturk@gmail.com</span></a>
            </div>
        </li>
        <li><a href="#!"><i class="material-icons">cloud</i>ADMINISTRACIÓN</a></li>
        <li><a href="#!">VER PARCHE</a></li>
        <li><a href="#!"><i class="material-icons">add_location</i>CREAR PARCHE</a></li>
        <li><div class="divider"></div></li>
        <li><a class="waves-effect" href="#!">Ayuda</a></li>
    </ul>
    
    @*Html.ActionLink(" Quien Se Pega !!", "Index", "Home", new { area = "" }, new { @class = "brand-logo" })*@

    <!--Navbar-->
    <nav class="navbar-dark orange">
        <div class="nav-wrapper">
            <a href="#" data-activates="slide-out" id="button-collapse" class="brand-logo"><i class="material-icons medium icon-demo">menu</i>Quien Se Pega !!</a>
            <ul class="right hide-on-med-and-down">
                <li><a href="mobile.html"><i class="material-icons">settings_power</i></a></li>
            </ul>
        </div>
    </nav>

    <!--/.Navbar-->



    <div class="container">
        <br />
        @RenderBody()

        <hr />

    </div>

    <footer class="page-footer orange lighten-2">
        <div class="container">
            <div class="row">
                <div class="col l6 s12">
                    <h5 class="white-text">Construido Por</h5>
                    <div class="chip">
                        <img src="@Url.Content("~/img/Unipanamericana.jpg")" alt="Desarrollador">
                        Alex Cifuentes Sánchez
                    </div>
                    <div class="chip">
                        <img src="@Url.Content("~/img/Unipanamericana.jpg")" alt="Desarrollador">
                        Comp 2
                    </div>
                    <div class="chip">
                        <img src="@Url.Content("~/img/Unipanamericana.jpg")" alt="Desarrollador">
                        Comp 3
                    </div>
                    <div class="chip">
                        <img src="@Url.Content("~/img/Unipanamericana.jpg")" alt="Desarrollador">
                        Comp 4
                    </div>
                </div>
                <div class="col l4 offset-l2 s12">
                    <h5 class="white-text">Hecho con AMOR gracias a </h5>
                    <p class="grey-text text-lighten-4">Materialize y Boostrap.</p>
                </div>
            </div>
        </div>
        <div class="footer-copyright">
            <div class="container">
                <p>&copy; @DateTime.Now.Year - PARCEROS S.A.</p>               
            </div>
        </div>
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/materializejs")
    @Scripts.Render("~/bundles/homejs")
    @RenderSection("scripts", required: false)
</body>
</html>
