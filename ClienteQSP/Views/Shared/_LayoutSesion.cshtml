<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>

    <!--Navbar-->
    <nav class="wrap navbar navbar-expand-lg navbar-dark orange">

        <!-- Navbar brand -->
        <img src="https://mdbootstrap.com/img/logo/mdb-transparent.png" height="30" class="d-inline-block align-top" alt="">
        @Html.ActionLink(" Quien Se Pega !!", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })

    </nav>
    <!--/.Navbar-->
             
                
    
    <div class="container">
        <br />
            @RenderBody()
        
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - PARCEROS S.A.</p>
            </footer>
        
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/mdb")
    @Scripts.Render("~/bundles/loginval")
    @RenderSection("scripts", required: false)
</body>
</html>
